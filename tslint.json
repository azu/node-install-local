{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "single",
            "jsx-double"
        ],
        "arrow-parens": [
            false
        ],
        "object-literal-sort-keys": false,
        "no-unused-expression": [
            false
        ],
        "trailing-comma": [
            false
        ],
        "interface-name": [
            false
        ],
        "no-console": [
            false
        ],
        "max-line-length": [
            true,
            160
        ],
        "ban-types": [
            false
        ]
    },
    "rulesDirectory": []
}